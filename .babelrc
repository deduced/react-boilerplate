{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    [
      "@emotion/babel-preset-css-prop",
      {
        "sourceMap": false
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./src/"],
        "extensions": [".js", ".json"],
        "alias": {
          "assets": "./src/assets",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
